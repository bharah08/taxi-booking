pipeline{
    agent any
    tools{
        maven 'maven'
    }
    stages{
        stage('gi-checkout'){
            steps{
                git 'https://github.com/bharah08/taxi-booking.git'
            }
        }
        stage('deploy-k8s'){
            steps{
                script{
                kubeconfig(credentialsId: 'k8s', serverUrl: '') {
    sh '(kubectl apply -f deploy.yaml)'
                }
}
            }
        }
    }
}
