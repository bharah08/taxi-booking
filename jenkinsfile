pipeline{
    agent any
    tools{
        maven 'maven'
    }
    stages{
        stage('gi-checkout'){
            steps{
                git 'https://github.com/bharah08/taxi-booking.git'
            }
        }
        stage('deploy-k8s'){
            steps{
                script{
                withKubeConfig(caCertificate: '', clusterName: '', contextName: '', credentialsId: 'k8s', namespace: '', restrictKubeConfigAccess: false, serverUrl: '') {
sh 'kubectl apply -f deploy.yaml'
    
                }
}
            }
        }
    }
}
